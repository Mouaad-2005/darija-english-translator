{
  "manifest_version": 3,
  "name": "WISD DARIJA-ENGLISH TRANSALATOR",
  "version": "1.2",
  "permissions": ["sidePanel"],
  "host_permissions": [
    "http://localhost:8081/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Open Darija Translator"
  },
  "side_panel": {
    "default_path": "sidepanel.html"
  }
}